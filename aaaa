    <script>
        require([
                'jquery',
                'Mirasvit_SearchAutocomplete/js/autocomplete',
                'Mirasvit_SearchAutocomplete/js/typeahead'
            ], function ($, autocomplete, typeahead) {
                var selector = 'input#search, input#mobile_search, .minisearch input[type="text"]';

                $(document).ready(function () {
                    $('#search_mini_form').prop("minSearchLength", 10000);

                    var $input = $(selector);

                    $input.each(function (index, searchInput) {
                        
                        new autocomplete($(searchInput)).init({"query":"","priceFormat":{"pattern":"HKD %s","precision":2,"requiredPrecision":2,"decimalSymbol":".","groupSymbol":",","groupLength":3,"integerRequired":false},"minSearchLength":3,"url":"https:\/\/store.nintendo.com.hk\/searchautocomplete\/ajax\/suggest\/","storeId":"1","delay":300,"layout":null,"popularTitle":"[\u4eba\u6c23\u641c\u5c0b]","popularSearches":["Zelda","Nba","Mario","Fifa","Cuphead"],"isTypeaheadEnabled":null,"typeaheadUrl":"https:\/\/store.nintendo.com.hk\/searchautocomplete\/ajax\/typeahead\/","minSuggestLength":2});
                    });
                });
            }
        );
    </script>

